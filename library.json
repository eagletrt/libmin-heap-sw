{
  "$schema": "https://raw.githubusercontent.com/platformio/platformio-core/develop/platformio/assets/schema/library.json",
  "name": "Min-heap",
  "version": "1.0.0",
  "description": "This library implements all the functions used to create and manage a minimum heap data structure.",
  "keywords": [
    "min-heap",
    "heap",
    "embedded"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/eagletrt/libmin-heap-sw"
  },
  "dependencies": [
    {
      "name": "ArenaAllocator",
      "version": "https://github.com/eagletrt/libarena-allocator-sw.git"
    }
  ],
  "authors": [
    {
      "name": "Antonio Gelain",
      "email": "antonio.gelain2@gmail.com",
      "maintainer": true
    },
    {
      "name": "Dorijan Di Zepp",
      "email": "dorijan.dizepp@gmail.com",
      "maintainer": true
    }
  ],
  "license": "AGPL-3.0-only",
  "frameworks": "*",
  "platforms": "*",
  "headers": [
    "min-heap.h",
    "min-heap-api.h"
  ],
  "examples": [
    {
      "name": "Simple Min Heap Implementation",
      "src": ["examples/min-heap-basic.c"]
    },
    {
      "name": "Simple Min Heap Implementation with Error Handling",
      "src": ["examples/min-heap-error-handling.c"]
    }
  ],
  "export": {
    "include": [
      "src/**",
      "include/**"
    ]
  }
}
